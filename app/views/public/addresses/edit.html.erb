<div class="addess_edit">
    <h1>配送先登録/一覧</h1>
     <%= form_for @address do |f| %>
       <label for="addresse_postal_code">郵便番号(ハイフンなし)</label>
         <%= f.text_field :postel_code %><br>
       <label for="addresse_addresse">住所</label>
         <%= f.text_field :address %><br>
       <label for="addresse_receiver_name">宛名</label>
         <%= f.text_field :receiver_name %><br>
     <%= f.submit "変更内容を保存", class: "btn btn-addess-edit" %>
     <% end %>
</div>